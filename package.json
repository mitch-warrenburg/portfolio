  "scripts": {
    "start": "yarn run clean && parcel --no-source-maps src/index.html --port 1234",
    "start:debug": "NODE_ENV=debug yarn start",
    "build": "yarn clean && run-s build:*",
    "build:react": "yarn clean && parcel build --no-content-hash --detailed-report --no-cache --no-source-maps --out-dir build src/index.html",
    "build:processFiles": "node rename.js && cp src/assets/robots.txt build && cp src/assets/*.html build",
    "build:sw": "parcel build src/sw/sw.ts --no-source-maps --detailed-report --no-cache --out-dir build",
    "postbuild": "node src/sw/injectManifest.js",
    "clean": "rimraf .cache dist build *.log",
    "tidy": "run-s ts format && git add .",
    "ts": "tsc",
    "format": "run-p format:*",
    "format:code": "prettier --write 'src/**/*.{ts,jsx,tsx}'",
    "format:scss": "stylelint --color --fix './src/**/*.{scss,ts,tsx}'",
    "lint": "eslint --fix 'src/**/*.{js,ts,jsx,tsx}'"
  }
