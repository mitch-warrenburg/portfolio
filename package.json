  "scripts": {
    "start": "yarn run clean && parcel src/index.html --port 3000",
    "build": "parcel build src/index.html --detailed-report",
    "clean": "rimraf .cache dist",
    "tidy": "run-s ts format lint && git add .",
    "ts": "tsc",
    "format": "run-p format:*",
    "format:code": "prettier --write 'src/**/*.{js,ts,jsx,tsx}'",
